#include <iostream>
#include <string>

namespace morestd { 
    void println(std::string I) {
        std::cout << I;
        std::cout << "\n";
        

    }
    void print(std::string I) {
        std::cout << I;

    }
    void input(std::string I) {
        std::cin >> I;
    }
    void inputln(std::string I) {
        getline(std::cin, I);
    }
    class math {
        public: static int add(int a, int b) {
            return a + b;

        }
        public: static int sub(int a, int b) {
            return a - b;

        }
        public: static int mul(int a, int b) {
            return a * b;

        }
        public: static int div(int a, int b) {
            return a / b;

        }
    };
   
}